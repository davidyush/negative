.main
  margin-top 80px
.create-text
  resize:none
.form-text
  margin-top 10px
  font-size 16px
.nav-tabs,.nav-pills
  margin-bottom 20px
  overflow hidden

.iframe
  width:100%;
  height: 400px;

a:active,a:hover,a:focus,
input:focus,button:focus,
.nav-tabs>li.active>a:hover,
.nav-tabs>li.active>a:focus:hover
  outline none
  overflow hidden

@media only screen and (max-width:360px)
  .iframe
    width 100%
    height 250px
